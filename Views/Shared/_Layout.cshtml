<body>

    <nav id="colorlib-main-nav" role="navigation">
        <a href="#" class="js-colorlib-nav-toggle colorlib-nav-toggle active"><i></i></a>
        <div class="js-fullheight colorlib-table">
            <div class="colorlib-table-cell js-fullheight">                
                <div class="row">
                    <div class="col-md-12 text-center">
                        <ul>
                            <li class="active"><a href="/">Home</a></li>
                            <li><a href="/#ourmenu">Menu</a></li>
                            <li><a href="/ServiceHour">Open Hours</a></li>
                            @*<li><a href="/Gallery">Gallery</a></li>*@
                            <li><a href="/AboutUs">About us</a></li>
                            <li><a href="/Contact">Contact</a></li>
                            <li><hr/></li>
                            <li><a href="/Login">Login</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </nav>

    <div id="colorlib-page">
        <header>
            <div class="container">
                @if(!string.IsNullOrEmpty(string.Format("{0}",TempData.Peek("Announcement"))))
                {
                    <div class="alert alert-success">@string.Format("{0}",TempData["Announcement"])
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                }
                <div class="row">
                    <div class="col-md-12">
                        <div class="colorlib-navbar-brand logo">
                            <a href="/"><img src="~/images/logo.png" height="150" width="310" class="desktop"></a>
                        </div>
                        <a href="#" class="js-colorlib-nav-toggle colorlib-nav-toggle"><i></i></a>
                    </div>
                </div>
            </div>
        </header>
        @RenderBody()

        <footer>
            <div id="footer">
                <div class="container">
                    <div class="row">
                        <div class="col-md-3 col-pb-sm">
                            <h2>Himalayan Shangrila Kitchen</h2>
                            <p>We are a family-owned restaurant with a passion for sharing our most treasured meals from Nepal. Our spices are homemade and we source our produce from local farmers' markets whenever possible.</p>
                            <p class="colorlib-social-icons">
                                <a href="https://fb.com/shangrilahimalayan/" target="_blank"><i class="icon-facebook4"></i></a>
                                <a href="#"><i class="icon-instagram"></i></a>
                                <a href="https://www.yelp.com/biz/shangrila-himalayan-kitchen-south-lake-tahoe" target="_blank"><i class="icon-yelp"></i></a>
                            </p>
                        </div>
                        <div class="col-md-6 col-pb-sm">
